<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jcse.app.egr2" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigateActivity"
            android:label="@string/title_activity_navigate"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="jcse.app.egr2.MainActivity" />
        </activity>
        <activity
            android:name=".ClassroomActivity"
            android:label="@string/title_activity_classroom"
            android:parentActivityName=".NavigateActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="jcse.app.egr2.NavigateActivity" />
        </activity>
    </application>

</manifest>
